# Environment Variables for Frontend

# API Configuration
# Base URL for the Laravel backend API
VITE_API_BASE_URL=your_laravel_application_base_url/api

# Laravel Application Base URL (without /api)
LARAVEL_APP_BASE_URL=your_laravel_application_base_url

# Environment Configuration
# Set to 'development' for local development, 'production' for production
REACT_APP_ENVIRONMENT=development

# App Configuration
# Name of your application
REACT_APP_APP_NAME=BuddyScript

# App version
REACT_APP_APP_VERSION=1.0.0